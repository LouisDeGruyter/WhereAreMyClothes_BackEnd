{
  "name": "webservices-Louis-De-Gruyter",
  "version": "1.0",
  "jest": {
    "testPathIgnorePatterns": [
      "node_modules",
      "src",
      "config",
      "models",
      "migrations",
      "seeders"
    ],
    "testMatch": [
      "**/__tests__/**/*.spec.js"
    ]
  },
  "scripts": {
    "start": "npx env-cmd nodemon src/index.js",
    "start:prod": "node src/index.js",
    "seed:prepare": "sequelize db:drop && sequelize db:create && yarn start",
    "seed": "sequelize db:seed --seed 202211271307_users && sequelize db:seed --seed 20221127121254-kleerkasten && sequelize db:seed --seed 20221127121542-kledingstukken",
    "test": "npx env-cmd -f .env.test jest --runInBand",
    "test:coverage": "yarn test --coverage",
    "migrate": "sequelize db:migrate"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "axios": "1.1.3",
    "config": "^3.3.8",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "joi": "^17.7.0",
    "jwks-rsa": "^3.0.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-jwt": "^4.0.3",
    "mysql2": "^2.3.3",
    "node-emoji": "^1.11.0",
    "path": "^0.12.7",
    "sequelize": "^6.25.8",
    "sequelize-cli": "^6.5.2",
    "serialize-error": "8.1.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.1"
  }
}
